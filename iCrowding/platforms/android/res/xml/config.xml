    <vs:plugin name="cordova-plugin-keyboard" src="https://github.com/cjpearson/cordova-plugin-keyboard.git" version="1.1.2" />
    <vs:plugin name="org.apache.cordova.file-transfer" version="0.5.0" />
    <vs:plugin name="org.apache.cordova.camera" version="0.3.6" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.12" />
    <vs:plugin name="org.apache.cordova.contacts" version="" />
